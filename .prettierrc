
{
    "semi": false,
    "singleQuote": false,
    "trailingComma": "none",
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "bracketSpacing": true,
    "plugins": ["prettier-plugin-solidity"],
    "overrides": [
      {
        "files": "*.sol",
        "options": {
          "parser": "solidity-parse",
          "printWidth": 120,
          "tabWidth": 4,
          "useTabs": false,
          "singleQuote": false,
          "bracketSpacing": true,
          "explicitTypes": "always"
        }
      }
    ]
  }